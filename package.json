{
  "name": "pg-mate",
  "description": "Pg-mate is a migration management tool for PostgreSQL databases",
  "version": "1.0.19",
  "repository": "https://github.com/flodlc/pg-mate.git",
  "author": "flodlc <f.delacomble@gmail.com>",
  "license": "MIT",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "module": "./dist/index.mjs",
  "type": "commonjs",
  "private": false,
  "files": [
    "dist/*"
  ],
  "scripts": {
    "build": "tsup src/index.ts --dts",
    "dev": "tsup src/index.ts --dts --watch ."
  },
  "peerDependencies": {
    "pg": ">=7.18.2",
    "typescript": ">=4.1"
  },
  "devDependencies": {
    "@types/pg": "^8.6.6",
    "pg": "^8.9.0",
    "tsup": "^6.6.3",
    "typescript": "^4.9.5"
  },
  "keywords": [
    "postgresql",
    "postgres",
    "migrations",
    "pg"
  ]
}
